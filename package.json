{
  "name": "web3-vue",
  "version": "0.0.1",
  "description": "Crypto wallet login/integration for Vue 3. Currently supporting the Solana blockchain.",
  "keywords": [
    "component",
    "library",
    "vue",
    "typescript",
    "javascript",
    "solana",
    "blockchain",
    "web3",
    "nfts"
  ],
  "license": "MIT",
  "author": "Open Crypto Labs",
  "main": "./dist/web3-vue.umd.js",
  "module": "./dist/web3-vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/web3-vue.es.js",
      "require": "./dist/web3-vue.umd.js"
    },
    "./styles.css": "./src/styles/index.css",
    "./postcss.config.js": "./postcss.config.js"
  },
  "files": [
    "dist",
    "src",
    "styles.css",
    "postcss.config.js"
  ],
  "typings": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite --port 3333 --open",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "vitebook:dev": "vitebook dev",
    "vitebook:build": "vitebook build",
    "vitebook:preview": "vitebook preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:e2e": "cypress open",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --emitDeclarationOnly"
  },
  "peerDependencies": {
    "@solana/wallet-adapter-wallets": "^0.16"
  },
  "dependencies": {
    "@solana/wallet-adapter-base": "^0.9.3",
    "@solana/web3.js": "^1.32.1",
    "pinia": "^2.0.13",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.21.1",
    "@babel/runtime": "7.17.9",
    "@babel/types": "7.17.0",
    "@headlessui/vue": "1.6.0",
    "@heroicons/vue": "1.0.6",
    "@project-serum/anchor": "0.24.2",
    "@solana/wallet-adapter-wallets": "0.16.0",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.5.0",
    "@tailwindcss/line-clamp": "0.3.1",
    "@tailwindcss/typography": "0.5.2",
    "@types/keccak": "3.0.1",
    "@types/node": "17.0.25",
    "@types/readable-stream": "2.3.13",
    "@vitejs/plugin-vue": "2.3.1",
    "@vue/compiler-sfc": "3.2.33",
    "@vueuse/core": "8.3.1",
    "autoprefixer": "10.4.5",
    "eslint": "8.14.0",
    "eslint-plugin-cypress": "2.12.1",
    "pnpm": "6.32.10",
    "postcss": "8.4.12",
    "postcss-import": "14.1.0",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.10",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.3",
    "unplugin-auto-import": "0.7.1",
    "unplugin-vue-components": "0.19.3",
    "vite": "2.9.6",
    "vitest": "0.10.0",
    "vue-tsc": "0.34.10"
  },
  "engines": {
    "node": ">=16.*"
  },
  "contributors": [
    "Chris Breuer <chris@meema.xyz>"
  ]
}
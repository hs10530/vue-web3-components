{
  "name": "web3-vue",
  "version": "0.0.6",
  "type": "module",
  "description": "The easy way to integrate web3 technologies with Vue 3. Currently supporting the Solana blockchain.",
  "keywords": [
    "component",
    "library",
    "vue",
    "typescript",
    "javascript",
    "solana",
    "blockchain",
    "web3",
    "nfts",
    "dao"
  ],
  "license": "MIT",
  "author": "Open Web Labs",
  "main": "./dist/web3-vue.umd.js",
  "module": "./dist/web3-vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/web3-vue.es.js",
      "require": "./dist/web3-vue.umd.js"
    },
    "./styles.css": "./src/styles/index.css",
    "./postcss.config.js": "./postcss.config.js"
  },
  "files": [
    "dist",
    "src",
    "styles.css",
    "postcss.config.js"
  ],
  "typings": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite --port 3333 --open",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "commit": "git add . && commit",
    "test:unit": "vitest",
    "test:e2e": "cypress open",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --emitDeclarationOnly",
    "preinstall": "node .scripts/check-for-pnpm.js && node .scripts/check-node-version.js",
    "prepare": "husky install",
    "release:dry": "standard-version --dry-run",
    "release": "standard-version",
    "publish": "release && git push --follow-tags && pnpm publish"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,vue,md,json}": "prettier --write"
  },
  "peerDependencies": {
    "@solana/wallet-adapter-wallets": "^0.16"
  },
  "dependencies": {
    "@solana/wallet-adapter-base": "^0.9.5",
    "@solana/web3.js": "^1.43.1",
    "vue": "^3.2.35"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.23.1",
    "@babel/runtime": "7.18.0",
    "@babel/types": "7.18.0",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@commitlint/prompt-cli": "^16.3.0",
    "@headlessui/vue": "1.6.2",
    "@heroicons/vue": "1.0.6",
    "@project-serum/anchor": "0.24.2",
    "@solana/wallet-adapter-wallets": "0.16.1",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/line-clamp": "0.4.0",
    "@tailwindcss/typography": "0.5.2",
    "@types/keccak": "3.0.1",
    "@types/node": "17.0.25",
    "@types/readable-stream": "2.3.13",
    "@vitejs/plugin-vue": "2.3.3",
    "@vue/compiler-sfc": "3.2.35",
    "@vueuse/core": "8.5.0",
    "autoprefixer": "10.4.7",
    "eslint": "8.16.0",
    "eslint-plugin-cypress": "2.12.1",
    "husky": "^8.0.1",
    "pnpm": "7.1.2",
    "postcss": "8.4.14",
    "postcss-import": "14.1.0",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.11",
    "standard-version": "^9.3.2",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.4",
    "unplugin-auto-import": "0.7.2",
    "unplugin-vue-components": "0.19.5",
    "vite": "2.9.9",
    "vitest": "0.12.9",
    "vue-tsc": "0.34.15"
  },
  "engines": {
    "node": ">=v16.15.0",
    "pnpm": ">=7.1.2"
  },
  "contributors": [
    "Chris Breuer <chris@meema.xyz>"
  ]
}
